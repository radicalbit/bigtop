set -ex

. `dirname $0`/bigtop.bom

echo "Later we'll be running a real build here.\n \
For the time being just pretend we did"

echo "Now we'll be just unpacking a pre-built binary tarball"

tar zxf ~/work/bigtop/hawq-2.0.0-incubating.tgz

echo "Pre-built binaries are unpacked"
